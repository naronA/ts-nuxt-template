{
  "name": "app",
  "version": "1.0.0",
  "description": "My magnificent Nuxt.js project",
  "author": "tminami",
  "private": true,
  "scripts": {
    "dev": "nuxt",
    "build": "nuxt build",
    "start": "nuxt start",
    "generate": "nuxt generate",
    "lint": "eslint --ext .js,.vue,.ts --ignore-path .eslintignore .",
    "lintfix": "eslint --fix --ext .js,.vue,.tx --ignore-path .eslintignore ."
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.js": [
      "eslint --ext .js --fix --ignore-path .eslintignore .",
      "git add"
    ],
    "*.vue": [
      "eslint --ext .vue --fix --ignore-path .eslintignore .",
      "git add"
    ],
    "*.ts": [
      "eslint --ext .ts --fix --ignore-path .eslintignore .",
      "git add"
    ]
  },
  "dependencies": {
    "@nuxtjs/axios": "^5.3.6",
    "@nuxtjs/bulma": "^1.2.0",
    "cross-env": "^5.2.0",
    "nuxt": "2.3.4",
    "lodash": "^4.17.11",
    "nuxt-property-decorator": "^1.3.1",
    "vuex-class": "^0.3.1"
  },
  "devDependencies": {
    "acorn": "^6.0.5",
    "babel-eslint": "^8.2.1",
    "eslint": "^5.0.1",
    "eslint-config-prettier": "^3.4.0",
    "eslint-config-standard": "^12.0.0",
    "eslint-config-typescript": "^1.1.0",
    "eslint-loader": "^2.0.0",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-node": "^8.0.1",
    "eslint-plugin-prettier": "2.6.2",
    "eslint-plugin-promise": "^4.0.1",
    "eslint-plugin-react": "^7.12.3",
    "eslint-plugin-standard": "^4.0.0",
    "eslint-plugin-typescript": "^0.14.0",
    "eslint-plugin-vue": "^5.1.0",
    "husky": "^1.3.1",
    "lint-staged": "^8.1.0",
    "node-sass": "^4.11.0",
    "nodemon": "^1.11.0",
    "prettier": "1.14.3",
    "sass-loader": "^7.1.0",
    "ts-loader": "5.3.3",
    "typescript": "3.1.1",
    "typescript-eslint-parser": "^21.0.2",
    "vue-eslint-parser": "^5.0.0",
    "vuex-module-decorators": "^0.9.7"
  }
}
